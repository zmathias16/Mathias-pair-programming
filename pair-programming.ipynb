   "source": [
    "def mean_offset(data, offset):\n",
    "    # sum variable\n",
    "    total = 0\n",
    "    for a in data:\n",
    "        total += a\n",
    "    \n",
    "    mean = total / len(data)\n",
    "    \n",
    "    # offsets the mean by the offset\n",
    "    offset_mean = mean + offset\n",
    "    \n",
    "    return offset_mean\n",
    "    "
   ]
  }
